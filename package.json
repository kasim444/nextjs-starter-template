{
    "name": "nextjs-starter-template",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "yarn run clean && yarn && next dev -p 3005",
        "build": "next build",
        "postbuild": "next-sitemap --config next-sitemap-config.js",
        "start": "next start",
        "clean": "npx rimraf .next",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook -o docs-build",
        "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
        "type-check": "tsc",
        "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
        "generate": "yarn plop --plopfile ./generators/plopfile.js",
        "gen:theme-typings": "chakra-cli tokens ./src/theme/index.ts",
        "prepare-theme": "yarn run gen:theme-typings"
    },
    "dependencies": {
        "@chakra-ui/icons": "^1.1.1",
        "@chakra-ui/react": "^1.7.2",
        "@chakra-ui/theme-tools": "^1.3.1",
        "@emotion/react": "^11",
        "@emotion/styled": "^11",
        "@hookform/resolvers": "^2.8.1",
        "axios": "^0.21.1",
        "classnames": "^2.3.1",
        "framer-motion": "^5.3.3",
        "mobx": "^6.0.4",
        "mobx-react-lite": "^3.2.2",
        "mobx-state-tree": "^5.0.1",
        "next": "^12.0.4",
        "next-compose-plugins": "^2.2.1",
        "next-seo": "^4.28.1",
        "next-translate": "^1.0.1",
        "nextjs-progressbar": "^0.0.13",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-hook-form": "^7.16.2",
        "swiper": "^7.3.2",
        "uuid": "^8.3.2",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@chakra-ui/cli": "^1.6.0",
        "@storybook/addon-actions": "^6.4.7",
        "@storybook/addon-controls": "^6.4.7",
        "@storybook/addon-essentials": "^6.4.7",
        "@storybook/addon-links": "^6.4.7",
        "@storybook/addon-storysource": "^6.4.7",
        "@storybook/addon-viewport": "^6.4.7",
        "@storybook/builder-webpack5": "^6.4.7",
        "@storybook/manager-webpack5": "^6.4.7",
        "@storybook/react": "^6.4.7",
        "@types/cookie": "^0.4.1",
        "@types/js-cookie": "^3.0.0",
        "@types/marked": "^3.0.1",
        "@types/node": "^14.14.13",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-paginate": "^7.1.1",
        "@types/react-text-truncate": "^0.14.1",
        "@types/uuid": "^8.3.3",
        "@typescript-eslint/eslint-plugin": "^4.9.1",
        "@typescript-eslint/parser": "^4.9.1",
        "babel-loader": "^8.2.3",
        "eslint": "^7.15.0",
        "eslint-config-prettier": "^7.0.0",
        "eslint-plugin-prettier": "^3.2.0",
        "husky": "^4.3.7",
        "lint-staged": "^10.5.3",
        "next-sitemap": "^1.6.203",
        "plop": "^2.7.4",
        "prettier": "^2.5.1",
        "storybook-addon-state": "^1.0.3",
        "tsconfig-paths-webpack-plugin": "^3.5.1",
        "typescript": "^4.1.3"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "yarn run lint -- --quiet --fix"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
